    {
        "id": "send_processed_data",
        "type": "function",
        "z": "plantguard_flow",
        "name": "Format Sensor Data",
        "func": "// Extract all sensor fields from payload\nconst p = msg.payload;\n\n// Build complete payload with case-insensitive field mapping\nmsg.payload = {\n    nodeId: msg.nodeId || p.nodeId,\n    source: 'node-red',\n    // Core sensor readings (handle both uppercase and lowercase)\n    temperature: p.temperature || p.Temperature,\n    soilMoisture: p.soilMoisture || p.SoilMoisture,\n    ph: p.ph || p.pH,  // Handle both \"ph\" and \"pH\"\n    humidity: p.humidity || p.Humidity,\n    lightIntensity: p.lightIntensity || p.LightIntensity,\n    // NPK sensors\n    nitrogen: p.nitrogen || p.Nitrogen,\n    phosphorus: p.phosphorus || p.Phosphorus,\n    potassium: p.potassium || p.Potassium,\n    // Other measurements\n    electricalConductivity: p.electricalConductivity || p.ElectricalConductivity,\n    // Power metrics\n    batteryLevel: p.batteryLevel || p.BatteryLevel\n};\n\nreturn msg;",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 960,
        "y": 100,
        "wires": [
