    {
        "id": "parse_sensor_data",
        "type": "function",
        "z": "plantguard_flow",
        "name": "Extract Node ID",
        "func": "// Extract node ID from MQTT topic\nconst topicParts = msg.topic.split('/');\nmsg.nodeId = topicParts[1]; // esp32/SLAVE_001/sensors -> SLAVE_001\n\n// Add timestamp if not present\nif (!msg.payload.timestamp) {\n    msg.payload.timestamp = new Date().toISOString();\n}\n\nreturn msg;",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 390,
        "y": 100,
        "wires": [
            [
                "check_thresholds",
                "send_processed_data"
            ]
        ]
    },
    {
        "id": "check_thresholds",
        "type": "function",
        "z": "plantguard_flow",
        "name": "Check Sensor Thresholds",
        "func": "const payload = msg.payload;\nconst nodeId = msg.nodeId;\nconst alerts = [];\n\n// pH threshold check (6.0 - 7.5)\nif (payload.ph !== null && payload.ph !== undefined) {\n    if (payload.ph < 6.0) {\n        alerts.push({\n            nodeId: nodeId,\n            type: 'warning',\n            title: 'Low pH Alert',\n            message: `pH is ${payload.ph.toFixed(2)} - Below safe range (6.0-7.5)`\n        });\n    } else if (payload.ph > 7.5) {\n        alerts.push({\n            nodeId: nodeId,\n            type: 'warning',\n            title: 'High pH Alert',\n            message: `pH is ${payload.ph.toFixed(2)} - Above safe range (6.0-7.5)`\n        });\n    }\n}\n\n// Soil moisture threshold check (40-80%)\nif (payload.soilMoisture !== null && payload.soilMoisture !== undefined) {\n    if (payload.soilMoisture < 40) {\n        alerts.push({\n            nodeId: nodeId,\n            type: 'critical',\n            title: 'Low Soil Moisture',\n            message: `Soil moisture is ${payload.soilMoisture}% - Plant needs watering!`\n        });\n    } else if (payload.soilMoisture > 80) {\n        alerts.push({\n            nodeId: nodeId,\n            type: 'warning',\n            title: 'High Soil Moisture',\n            message: `Soil moisture is ${payload.soilMoisture}% - Risk of overwatering`\n        });\n    }\n}\n\n// Temperature threshold check (18-30°C)\nif (payload.temperature !== null && payload.temperature !== undefined) {\n    if (payload.temperature < 18) {\n        alerts.push({\n            nodeId: nodeId,\n            type: 'warning',\n            title: 'Low Temperature',\n            message: `Temperature is ${payload.temperature}°C - Too cold for optimal growth`\n        });\n    } else if (payload.temperature > 30) {\n        alerts.push({\n            nodeId: nodeId,\n            type: 'warning',\n            title: 'High Temperature',\n            message: `Temperature is ${payload.temperature}°C - Too hot for optimal growth`\n        });\n    }\n}\n\n// Battery threshold check\nif (payload.batteryLevel !== null && payload.batteryLevel !== undefined) {\n    if (payload.batteryLevel < 20) {\n        alerts.push({\n            nodeId: nodeId,\n            type: 'info',\n            title: 'Low Battery',\n            message: `Battery level is ${payload.batteryLevel}% - Consider charging soon`\n        });\n    }\n}\n\n// If alerts exist, send them\nif (alerts.length > 0) {\n    msg.alerts = alerts;\n    return msg;\n}\n\nreturn null;",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 640,
        "y": 160,
        "wires": [
            [
                "send_alerts"
            ]
        ]
    },
    {
        "id": "send_alerts",
        "type": "function",
        "z": "plantguard_flow",
        "name": "Format Alert Requests",
        "func": "const alerts = msg.alerts;\nconst messages = [];\n\n// Create a separate message for each alert\nfor (const alert of alerts) {\n    messages.push({\n        payload: alert,\n        url: msg.apiUrl || 'YOUR_REPLIT_APP_URL/api/node-red/alert'\n    });\n}\n\nreturn [messages];",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 880,
        "y": 160,
        "wires": [
            [
                "http_send_alert"
            ]
        ]
    },
